{
"name": "darwinfi-backend",
"version": "1.0.0",
"description": "Backend API server for DarwinFi strategy management system",
"main": "dist/server.js",
"scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:injective": "jest --testPathPattern=injective",
    "typecheck": "tsc --noEmit",
    "deploy:testnet": "node scripts/deploy.js testnet",
    "deploy:mainnet": "node scripts/deploy.js mainnet"
}
"dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "winston": "^3.11.0",
    "morgan": "^1.10.0",
    "mongoose": "^8.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.1.4",
    "joi": "^17.11.0",
    "http-status-codes": "^2.3.0",
    "axios": "^1.6.2",
    "uuid": "^9.0.1",
    "socket.io": "^4.7.2",
    "moment": "^2.29.4",
    "@injectivelabs/sdk-ts": "^1.14.33",
    "@injectivelabs/networks": "^1.14.33",
    "@injectivelabs/ts-types": "^1.14.33",
    "eliza": "github:elizaOS/eliza"
},
"devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.4",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/mongoose": "^5.11.97",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.10",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
},
"engines": {
    "node": ">=18.0.0"
}
}

